  <label for="inputEmail3" class="col-sm-2 col-form-label">Add New Question</label>
  <div class="col-sm-10">
    <button type="button" class="btn btn-secondary">Add Question</button>
  </div>
</div>
<div class="d-none" id="question-template">
  <%- include('questionEditingTemplates/baseTemplate') %>
</div>
